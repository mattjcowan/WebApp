    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="ServiceStack.Api.OpenApi" Version="5.4.0" />
    <PackageReference Include="ServiceStack.OrmLite.MySql" Version="5.4.0" />
    <PackageReference Include="ServiceStack.OrmLite.Postgresql" Version="5.4.0" />
    <PackageReference Include="ServiceStack.OrmLite.SqLite" Version="5.4.0" />
    <PackageReference Include="ServiceStack.OrmLite.SqlServer" Version="5.4.0" />
    <PackageReference Include="ServiceStack.Server" Version="5.4.0" />
